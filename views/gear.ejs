
<div class="container">
    <div class="col-sm-6">
    </div>
    <div class="col-sm-6">
      <h1 class="title">Shop for shoes</h1>
      <h1>Shoe Selection</h1>

      <% offeredGear.forEach(gear => { %>
      <h5>
        <%= gear.title %>
      </h5>

      <img src='<%= gear.img %>'>
      <input type="checkbox" id="selectGear" value="selectGear">
      <span>$
        <%= gear.cost %> </span>
      <% }); %>
    </div>
    <br>
    <br>
    <br>
    <br>
  </div>